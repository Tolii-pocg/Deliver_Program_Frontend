{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^uni-(.*)/uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$2/uni-$2.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"backgroundColor": "#fff",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/My/My",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/Order/Order",
			"style": {
				"navigationBarTitleText": "订单"
			}
		},
		{
			"path": "pages/TakeInfo/TakeInfo",
			"style": {
				"navigationBarTitleText": "取件信息"
			}
		},
		{
			"path": "pages/ReciveInfo/ReciveInfo",
			"style": {
				"navigationBarTitleText": "收货信息"
			}
		}
	],
	"tabBar": {
		"borderStyle": "black",
		"backgroundColor": "#fff",
		"color": "#000",
		"selectedColor": "#000",
		"list": [
			{
				"iconPath": "static/home.png",
				"selectedIconPath": "static/home-active.png",
				"text": "首页",
				"pagePath": "pages/index/index"
			},
			{
				"iconPath": "static/order.png",
				"selectedIconPath": "static/order-active.png",
				"text": "订单",
				"pagePath": "pages/Order/Order"
			},
			{
				"iconPath": "static/my.png",
				"selectedIconPath": "static/my-active.png",
				"text": "我的",
				"pagePath": "pages/My/My"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"titleNView": false
		}
	}
}